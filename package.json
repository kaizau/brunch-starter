{
  "name": "brunch-starter",
  "description": "A skeleton for general-purpose frontend projects.",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run clean && ./node_modules/brunch/bin/brunch watch --server --port 8888",
    "build": "BRUNCH_ENV=production npm run clean && ./node_modules/brunch/bin/brunch build",
    "clean": "rm -rf public/"
  },
  "dependencies": {
    "babel-brunch": "^6.1.1",
    "babel-preset-es2015": "^6.24.1",
    "brunch": "^2.10.9",
    "clean-css-brunch": "^2.10.0",
    "copyfilemon-brunch": "^3.3.0",
    "css-brunch": "^2.10.0",
    "digest-brunch": "^1.5.1",
    "envstatic-brunch": "^0.1.0",
    "eslint-brunch": "^3.12.0",
    "jade-static-brunch": "^1.8.4",
    "javascript-brunch": "^2.10.0",
    "jeet": "^7.0.0",
    "mithril": "^1.1.1",
    "normalize.css": "^7.0.0",
    "stylus-brunch": "^2.10.0",
    "uglify-js-brunch": "^2.10.0"
  }
}
